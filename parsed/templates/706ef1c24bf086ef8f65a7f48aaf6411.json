{"components/common/quick-search":"\r\n<form onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\r\n    <fieldset class=\"relative\" >\r\n            <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\r\n            <a href=\"#search\" class=\"clickable-search-icon icon-search\"><i class=\"fa fa-search\"></i></a>\r\n            <input class=\"form-control\" placeholder=\"Search your product\"\r\n                    data-search-quick\r\n                    name=\"{{name}}\"\r\n                    id=\"{{name}}\"\r\n                    data-error-message=\"{{lang 'search.error.empty_field'}}\"\r\n                    autocomplete=\"off\"\r\n            >\r\n    </fieldset>\r\n</form>\r\n<section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\r\n<p role=\"status\"\r\n    aria-live=\"polite\"\r\n    class=\"aria-description--hidden\"\r\n    data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\r\n></p>\r\n"}
