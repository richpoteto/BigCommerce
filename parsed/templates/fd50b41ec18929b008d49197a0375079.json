{"components/common/breadcrumbs":"<div class=\"breadcrumb-wrapper-main\">\r\n    <div class=\"breadcrumb-wrapper\">\r\n        <div class=\"breadcrumb-container\">\r\n            <nav aria-label=\"Breadcrumb\" class=\"breadcrumb-menu\">\r\n                <ol class=\"breadcrumbs\" itemscope itemtype=\"https://schema.org/BreadcrumbList\">\r\n                    {{#unless theme_settings.hide_breadcrumbs }}\r\n                    {{#each breadcrumbs}}\r\n                    <li class=\"breadcrumb {{#if @last}}is-active{{/if}}\" itemprop=\"itemListElement\" itemscope\r\n                        itemtype=\"https://schema.org/ListItem\">\r\n                        <a class=\"breadcrumb-label\" itemprop=\"item\" href=\"{{url}}\" {{#if @last}}aria-current=\"page\"\r\n                            {{/if}}>\r\n                            <span itemprop=\"name\">{{name}}</span>\r\n                        </a>\r\n                        <meta itemprop=\"position\" content=\"{{add @index 1}}\" />\r\n                    </li>\r\n                    {{/each}}\r\n                    {{/unless}}\r\n                </ol>\r\n            </nav>\r\n\r\n        </div>\r\n        <div class=\"breadcrumbs-img\">\r\n            <img src=\"{{cdn 'assets/img/header-logo.png'}}\" alt=\"Shop medical supplies from MeddMax\" />\r\n        </div>\r\n\r\n    </div>\r\n</div>\r\n"}
