{"components/common/navigation-list":"\r\n{{#if children}}\r\n<!-- <a href=\"/shop-all\">Shop by Categories</a> -->\r\n<a class=\"{{#if is_active}} activePage{{/if}}\"\r\n   href=\"{{url}}\"\r\n   data-collapsible=\"navPages-{{id}}\"\r\n>\r\n    {{name}}\r\n</a>\r\n\r\n<div class=\"dropdown-menu simple-dropdown\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n    <ul class=\"navPage-subMenu-list\">\r\n\t{{#if name '!=' 'Who we serve' }}\r\n        <li class=\"navPage-subMenu-item\">\r\n            <a class=\"navPage-subMenu-action navPages-action\"\r\n               href=\"{{url}}\"\r\n               aria-label=\"{{lang 'category.view_all.name' category=name}}\"\r\n            >\r\n                {{lang 'category.view_all.name' category=name}}\r\n            </a>\r\n        </li>\r\n\t\t{{/if}}\r\n        {{#each children}}\r\n            <li class=\"navPage-subMenu-item\">\r\n                {{#if children}}\r\n                    <a class=\"navPage-subMenu-action navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\"\r\n                       href=\"{{url}}\"\r\n                       aria-label=\"{{name}}\"\r\n                    >\r\n                     {{name}}\r\n                        <span class=\"collapsible-icon-wrapper\"\r\n                            data-collapsible=\"navPages-{{id}}\"\r\n                            data-collapsible-disabled-breakpoint=\"medium\"\r\n                            data-collapsible-disabled-state=\"open\"\r\n                            data-collapsible-enabled-state=\"closed\"\r\n                        >\r\n                            <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                                <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                            </i>\r\n                        </span>\r\n                    </a>\r\n                    <ul class=\"navPage-childList\" id=\"navPages-{{id}}\">\r\n                        {{#each 