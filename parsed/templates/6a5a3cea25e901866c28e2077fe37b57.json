{"components/common/forms/text":"<div class=\"form-field\" id=\"{{id}}\" data-validation=\"{{validation}}\" {{#if private_id}}data-type=\"{{private_id}}\"{{/if}}>\r\n    {{#if createAcc '!==' 1 }}\r\n\t<label class=\"form-label\" for=\"{{id}}_input\">{{label}}\r\n        {{#if required}}<small>{{lang 'common.required' }}</small>{{/if}}\r\n    </label>\r\n\t{{/if}}\r\n\t {{#if createAcc '==' 1 }}\r\n\t\r\n    <input type=\"text\"\r\n           name=\"{{name}}\"\r\n           id=\"{{id}}_input\"\r\n           class=\"form-input\"\r\n           data-label=\"{{label}}\"\r\n           data-input\r\n           aria-required=\"{{required}}\"\r\n           {{#if value}} value=\"{{value}}\"\r\n\t\t   {{else}}\r\n\t\t   {{#if label '==' 'Address Line 1'}} value=\"N/A\" {{/if}}\t\r\n\t\t   {{#if label '==' 'Suburb/City'}}  value=\"N/A\"  {{/if}}\r\n\t\t   {{#if label '==' 'State/Province'}}  value=\"N/A\"  {{/if}}\r\n\t\t   {{#if label '==' 'Zip/Postcode'}}  value=\"N/A\"  {{/if}}\r\n\t\t   {{/if}}\r\n           {{#if maxlength}}maxlength=\"{{maxlength}}\"{{/if}}\r\n\t\t   {{#if createAcc '!==' 1 }}\r\n           {{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}}\r\n\t\t   {{else}}\r\n\t\t   placeholder=\"{{label}}*\"\r\n\t\t   {{/if}}\r\n           {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\r\n\t\t   \r\n\t\t   \r\n\t\t   {{#if label '==' 'Address Line 1'}} style=\"display: none\" {{/if}}\r\n\t\t   {{#if label '==' 'Address Line 2'}} style=\"display: none\" {{/if}}\r\n\t\t   {{#if label '==' 'Suburb/City'}} style=\"display: none\" {{/if}}\r\n\t\t   {{#if label '==' 'State/Province'}} style=\"display: none\" {{/if}}\r\n\t\t   {{#if label '==' 'Zip/Postcode'}} style=\"display: none\" {{/if}}\r\n    >\r\n\t\r\n\t{{else}}\r\n\t <input type=\"text\"\r\n           name=\"{{name}}\"\r\n           id=\"{{id}}_input\"\r\n           class=\"form-input\"\r\n           data-label=\"{{label}}\"\r\n           data-input\r\n           aria-required=\"{{required}}\"\r\n           {{#if value}} value=\"{{value}}\"{{/if}}\r\n           {{#if maxlength}}maxlength=\"{{maxlength}}\"{{/if}}\r\n\t\t   {{#if createAcc '!==' 1 }}\r\n           {{#if placeholder}} placeholder=\"{{placeholder}}\"{{/if}}\r\n\t\t   {{else}}\r\n\t\t   placeholder=\"{{label}}*\"\r\n\t\t   {{/if}}\r\n           {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\r\n    >\r\n\t{{/if}}\r\n</div>\r\n"}
